{
  "name": "betterExperience",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Tweaks that may or may not break myExperience",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://myexperience.sfu.ca/myAccount/*"
  ],
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "https://myexperience.sfu.ca/myAccount/co-op/postings.htm"
      ],
      "js": [
        "src/js/content_coop.js"
      ]
    }
  ]
}